{
  "fileRoot": "src/ui/",
  "routes": [
    { "path": "/", "handler": "file", "content": "index.html" },
    { "path": "*", "handler": "file" },
    { "path": "/api/shutdown", "handler": "module", "module": "./src/api/server.js", "function": "stop" },
    { "path": "/api/version", "handler": "module", "module": "./src/api/info.js", "function": "version" }
  ]
}